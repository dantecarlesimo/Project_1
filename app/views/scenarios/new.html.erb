
<h1>Scenario</h1>

<div class="row">
    <div class="col-md-6 col-sm-6 col-md-offset-3 col-sm-offset-3">
    <%= form_for(@scenario) do |f| %>
  

      <%= f.label :Property_Address %>
      <%= f.text_field :title %><br>

      <%= f.label :income %>
      <%= f.text_field :income %><br>

      <%= f.label :housing_exp %>
      <%= f.text_field :housing_exp %><br>

      <%= f.label :other_exp %>
      <%= f.text_field :other_exp %><br>

      <%= f.label :credit_score %>
      <%= f.text_field :credit_score %><br>

      <%= f.label :purchase_price %>
      <%= f.text_field :purchase_price %><br>

      <%= f.label :loan_amount %>
      <%= f.text_field :loan_amount %><br>

      <%= f.label :rate %>
      <%= f.text_field :rate %><br>

        <%= f.label :program %>
      <%= f.text_field :program %><br>

      <%= f.submit "Create my Scenario", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
